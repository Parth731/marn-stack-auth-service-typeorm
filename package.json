{
  "name": "auth-services",
  "version": "1.0.0",
  "description": "the user management services",
  "main": "index.js",
  "scripts": {
    "start": "tsc && ts-node dist/server.js",
    "dev": "nodemon --legacy-watch src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "build": "tsc",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md,mjs}'",
    "format:check": "prettier --check .",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "exports": {
    "require": "./index.cjs",
    "import": "./index.js"
  },
  "author": "parth dangroshiya",
  "license": "ISC",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@jest/globals": "^29.7.0",
    "@tsconfig/node16": "^16.1.3",
    "@tsconfig/node22": "^22.0.0",
    "@types/express": "^5.0.0",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "nodemon": "^3.1.9",
    "prettier": "3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0",
    "winston": "^3.17.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md,mjs}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "express": "^4.21.2",
    "http-errors": "^2.0.0",
    "mongodb": "^5.9.2",
    "pg": "^8.13.1",
    "typeorm": "^0.3.20"
  }
}
